<header class='header_user'>
  <h1>Create a new user</h1>
  <h2>At this page you can add a new user with name, age and bio.</h2>
</header>
<main class='main'>
<%= form_with model: @user do |form| %>
  <div>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :age %><br>
    <%= form.text_field :age %>
  </div>

  <div>
    <%= form.label :biograph %><br>
    <%= form.text_area :biograph %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
</main>
